{
  "name": "workspace",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf node_modules .turbo dist logs",
    "db:dev": "pnpm db:generate & pnpm db:push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "dev": "tsnd --respawn --transpile-only --exit-child --watch src -r tsconfig-paths/register src/App.ts",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.3",
    "@grpc/proto-loader": "^0.7.3",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/microservices": "^9.1.6",
    "@prisma/client": "^4.5.0",
    "api-types": "workspace:*",
    "axios": "^1.1.3",
    "logger": "workspace:*",
    "nest-winston": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.4",
    "eslint-config-vectier": "workspace:*",
    "prisma": "^4.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "prisma": {
    "seed": "ts-node --transpile-only -r tsconfig-paths/register prisma/seed.ts"
  }
}
